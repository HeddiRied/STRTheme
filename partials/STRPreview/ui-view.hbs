{{#foreach posts visibility="all"}}

<div class="STRPreview">

<p>
	<time class="STRPreviewDate" datetime="{{date format='YYYY-MM-DD'}}">{{date format="MMMM Do, YYYY"}}</time>
</p>
<a class="STRPreviewTitle" href="{{url}}">{{title}}</a>
<iframe class="STRPreviewEmbed" width="100%" height="180" frameborder="no" scrolling="no" seamless src="https://go.rosano.ca/strolling-{{slug}}-{{#has tag="Video"}}video{{else}}audio{{/has}}"></iframe>
<p>
	<a class="STRPreviewNotes" href="{{url}}">â†’ {{slug}} episode notes</a>
</p>

</div>

{{/foreach}}
